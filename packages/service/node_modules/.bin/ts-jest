#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jiangsongyang/Documents/study/study-nest/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/jiangsongyang/Documents/study/study-nest/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/ts-jest@29.1.0_iq5n7lqt5lgobotj2eiby2n2b4/node_modules/ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/ts-jest@29.1.0_iq5n7lqt5lgobotj2eiby2n2b4/node_modules/ts-jest/cli.js" "$@"
fi
